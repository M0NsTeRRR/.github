{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "description": ["Process github actions"],
      "customType": "regex",
      "fileMatch": ["\\.ya?ml(\\.j2)?$"],
      "matchStrings": [
        '\\suses:\\s(?<depName>\\S+)@(?<currentValue>\\S+)'
      ],
      "datasourceTemplate": "github-releases",
    },
  ]
}
